<div class="container">
    <h1>DASHBOARD</h1>
    <div class="card-panel teal">
        <span class="white-text">
            <h6 id="qodQuote"></h3>
            <h6 id="qodAuthor"></h3>
        </span>
    </div>
    <div class="row">
        <div class="col s12 m5">
            <div class="card-panel teal">
            <h5>YOUR DOCUMENTS:</h5>
                <span class="white-text">

                    <ul>
                        {{#each document}}
                        {{>docs/doc-block}}
                        {{/each}}
                    </ul>
                </span>
                <a href="/newdoc"><button type="submit" class="btn btn-default white teal-text">Add HW/NOTE</button></a>
            </div>
        </div>
        <div class="col s12 m5">
            <div class="card-panel teal">
            <h5>TOP-GRADED DOCUMENTS:</h5>
                <span class="white-text">
                    
                    <ul>
                        {{#each topDocument}}
                        {{>docs/doc-block}}
                        {{/each}}
                    </ul>
                </span>
            </div>
        </div>
        <div class="col s12 m5">
            <div class="card-panel teal">
            <h5>YOUR TUTOR SESSIONS:</h5>
                <span class="white-text">
                    <ul>
                        {{#each session}}
                        {{>docs/session-block}}
                        {{/each}}
                    </ul>
                </span>
                                <a href="/schedule"><button type="submit" class="btn btn-default white teal-text">Add Tutor
                        Session</button></a>
            </div>
        </div>
    </div>
</div>
<script src="/js/dashboard.js"></script>