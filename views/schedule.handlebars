<div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <h2>Schedule Session:</h2>
                <form class="schedule">
                    <div class="form-group" style="display: none">
                        <label for="nameOfStudent">Student requesting help</label>
                        <input type="text" class="form-control" id="{{ user.id }}" placeholder="Homework Help">
                    </div>
                    <div class="form-group">
                        <label for="inputEventTitle">What do you want to work on?</label>
                        <input type="text" class="form-control" id="event-title-input" placeholder="Homework Help">
                    </div>
                    <div class="form-group">
                        <label for="selectDate">When do you want to work together?</label>
                        <div>
                            <!-- Element that gets date/time? -->
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="users">Who would you like to work with?</label>
                        <!-- Dropdown Trigger -->
                            <a class='dropdown-trigger btn' href='#' data-target='usersDropdown'>Select Tutor</a>

                            <!-- Dropdown Structure -->
                            <ul id='usersDropdown' class='dropdown-content'>
                            </ul>
                    </div>
                    <div style="display: none" id="alert" class="alert alert-danger" role="alert">
                        <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
                        <span class="sr-only">Error:</span> <span class="msg"></span>
                    </div>
                    <button type="submit" class="btn btn-default">Sign Up</button>
                
                </form>
                <br />
            </div>
        </div>
    </div>
      <script src="/js/schedule.js"></script>